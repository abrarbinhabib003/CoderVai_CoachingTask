{
  "name": "coaching-management-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js --watch . --ext js,json --ignore node_modules/"
  },
  "nodemonConfig": {
    "watch": [
      "*.js",
      "controllers/",
      "routes/",
      "middleware/",
      ".env"
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules/",
      "generated/",
      "prisma/migrations/"
    ],
    "delay": "1000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.1.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "prisma": "^6.11.1",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
